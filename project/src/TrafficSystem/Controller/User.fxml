<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="400.0"
            xmlns="http://javafx.com/javafx/2.2" fx:controller="TrafficSystem.Controller.UserController">
    <children>
        <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" text="&lt;"/>
        <Text layoutX="55.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Back to Dashboard"/>
        <VBox layoutX="104.0" layoutY="131.0" prefHeight="200.0" prefWidth="192.0">
            <children>
                <Button mnemonicParsing="false" onAction="#handleCardholder" prefHeight="71.0" prefWidth="192.0"
                        text="Cardholder"/>
                <Button mnemonicParsing="false" onAction="#handleVisitor" prefHeight="71.0" prefWidth="192.0"
                        text="Visitor"/>
            </children>
        </VBox>
    </children>
</AnchorPane>
